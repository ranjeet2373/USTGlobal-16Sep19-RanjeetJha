package revision;

import java.util.ArrayList;

public class List4 {
	
	public static void main(String[] args) {
		
		ArrayList al = new ArrayList<>();
		al.add(34.5);
		al.add(23.5);
		al.add(87.6);
		al.add(12.3);
		al.add(34.5);
		al.add(null);
		al.add(null);
		System.out.println(al);
		
		al.add(2, "ranjeet");
		System.out.println(al);
		
		al.remove(1);
		
		System.out.println("after removing "+al);
	}

}
